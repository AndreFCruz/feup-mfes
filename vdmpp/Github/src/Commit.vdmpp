class Commit

instance variables
	public hash: VDMUtils`String;
	public timestamp: Date;
	
	public author: User;

operations
	public Commit: VDMUtils`String * User * Date ==> Commit
	Commit(str, u, d) == (
		hash := str;
		author := u;
		timestamp := d;
		return self;
	)
	pre str <> []
	post hash = str;

end Commit